{
  "version": "1.0.0-alpha.6",
  "author": "Community for NL Design System",
  "description": "Mendix CSS for a design system based on the NL Design System architecture",
  "license": "EUPL-1.2",
  "name": "@gemeente-rotterdam/mendix-css",
  "keywords": [
    "nl-design-system"
  ],
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/rotterdam.git",
    "directory": "packages/mendix-css"
  },
  "exports": {
    "./dist/*.css": "./dist/*.css"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && node patch.mjs && rollup --config ./rollup.config.mjs",
    "clean": "rimraf dist/*"
  },
  "devDependencies": {
    "@gemeente-rotterdam/components-css": "workspace:*",
    "@gemeente-rotterdam/design-tokens": "workspace:*",
    "@mendix/atlas": "https://github.com/mendix/atlas/archive/refs/tags/atlas-web-content-v3.6.0.tar.gz",
    "@utrecht/badge-css": "1.4.1",
    "@utrecht/badge-data-css": "1.4.0",
    "@utrecht/breadcrumb-nav-css": "1.5.1",
    "@utrecht/button-css": "2.2.0",
    "@utrecht/custom-checkbox-css": "1.3.2",
    "@utrecht/focus-ring-css": "2.3.1",
    "@utrecht/form-label-css": "1.6.1",
    "@utrecht/heading-1-css": "1.5.1",
    "@utrecht/heading-2-css": "1.5.1",
    "@utrecht/heading-3-css": "1.5.1",
    "@utrecht/heading-4-css": "1.5.1",
    "@utrecht/heading-5-css": "1.5.1",
    "@utrecht/heading-6-css": "1.5.1",
    "@utrecht/heading-css": "1.4.1",
    "@utrecht/link-css": "1.6.1",
    "@utrecht/paragraph-css": "2.2.0",
    "@utrecht/radio-button-css": "1.6.1",
    "@utrecht/table-css": "1.6.1",
    "@utrecht/textarea-css": "2.3.2",
    "@utrecht/textbox-css": "1.5.0",
    "postcss-discard-duplicates": "7.0.2",
    "rimraf": "6.0.1",
    "rollup": "4.50.0",
    "rollup-plugin-postcss": "4.0.2"
  }
}
